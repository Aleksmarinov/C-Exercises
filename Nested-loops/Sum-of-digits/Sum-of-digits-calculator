string input = Console.ReadLine();

while (input != "End")
{
    int numbers = int.Parse(input);
    int sum = 0;

    while (numbers > 0)
    {
        int digit = numbers % 10;
        sum += digit;
        numbers /= 10;
    }

    Console.WriteLine($"Sum of digits = {sum}");

    input = Console.ReadLine();
}

Console.WriteLine("Goodbye");
