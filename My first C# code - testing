namespace testingCs
{
    class Program
    {
        static void Main(string[] args)
        {
            int runTime = 35;
            double runDistance = 3.5;

            if (runTime <= 30 && runDistance > 3.5)
            {
                Console.WriteLine("You are super fast");
            }
            else if (runTime >= 30 && runDistance < 3.5)
            {
                Console.WriteLine("You're not making your pace!");
            }
            else if (runTime > 30 && runDistance > 3)
            {
                Console.WriteLine("Nice workout");
            }
            else
            {
                Console.WriteLine("Keep on running");
            }
        }
    }
}
